{
  "ui:layout": {
    "component": "appbar",
    "transformData": "function(parent, def){let x = parent.props.context.props.formData.return;def.props['props-run'].disabled=!parent.emptyObj(x); return def}",
    "props": {
      "props-run": {
        "disabled": false,
        "id": "run-button"
      },
      "props-main": {
        "sx": {
          "height": "100vh",
          "flexGrow": 1,
          "py": 3,
          "pl": 3,
          "pr": "67px",
          "display": "flex",
          "flexDirection": "column"
        }
      },
      "tabs-props": [
      ],
      "disable-tabs": true,
      "children": [
        {
          "component": "ReflexContainer",
          "props": {
            "orientation": "horizontal",
            "children": [
              {
                "component": "ReflexElement",
                "transformData": "function(parent, def){def.props.flex = parent.emptyObj(parent.props.context.props.formData.return)?1:0.001; return def}",
                "props": {
                  "minSize": 50,
                  "flex": 1,
                  "style": {
                    "display": "flex",
                    "flex-direction": "column"
                  },
                  "children": [
                    {
                      "component": "AccordionSummary",
                      "props": {
                        "sx": {
                          "textAlign": "center",
                          "backgroundColor": "lightgray"
                        },
                        "style": {
                          "cursor": "unset"
                        },
                        "content": "INPUT"
                      }
                    },
                    {
                      "component": "Paper",
                      "props": {
                        "sx": {
                          "overflow": "auto",
                          "p": 1,
                          "m": 2
                        },
                        "children": [
                          {
                            "$id": "input",
                            "component": "Box",
                            "props": {
                              "uiSchema": {
                                "ui:layout": {
                                  "$id": "kwargs",
                                  "props": {
                                    "uiSchema": {
                                      "ui:layout": {
                                        "$id": "inputs",
                                        "props": {
                                          "uiSchema": {
                                            "ui:layout": {
                                              "component": "Grid",
                                              "props": {
                                                "container": true,
                                                "spacing": 2,
                                                "children": [
                                                  {
                                                    "component": "Grid",
                                                    "props": {
                                                      "item": true,
                                                      "xs": 6,
                                                      "children": [
                                                        {
                                                          "$id": "value_in"
                                                        }
                                                      ]
                                                    }
                                                  },
                                                  {
                                                    "component": "Grid",
                                                    "props": {
                                                      "item": true,
                                                      "xs": 6,
                                                      "children": [
                                                        {
                                                          "$id": "unit_in"
                                                        }
                                                      ]
                                                    }
                                                  },
                                                  {
                                                    "component": "Grid",
                                                    "props": {
                                                      "item": true,
                                                      "xs": 12,
                                                      "children": [
                                                        {
                                                          "$id": "units_out"
                                                        }
                                                      ]
                                                    }
                                                  }
                                                ]
                                              }
                                            }
                                          }
                                        }
                                      }
                                    }
                                  }
                                }
                              }
                            }
                          }
                        ]
                      }
                    }
                  ]
                }
              },
              {
                "component": "ReflexSplitter",
                "domain": "function(parent){let x = parent.props.context.props.formData.return; return !parent.emptyObj(x)}",
                "props": {
                  "style": {
                    "height": "unset"
                  },
                  "children": [
                    {
                      "component": "AccordionSummary",
                      "props": {
                        "sx": {
                          "backgroundColor": "lightgray"
                        },
                        "style": {
                          "cursor": "unset"
                        },
                        "content": "OUTPUT"
                      }
                    }
                  ]
                }
              },
              {
                "component": "ReflexElement",
                "domain": "function(parent){let x = parent.props.context.props.formData.return; return !parent.emptyObj(x)}",
                "props": {
                  "children": [
                    {
                      "component": "Paper",
                      "props": {
                        "sx": {
                          "p": 1,
                          "m": 2
                        },
                        "children": [
                          {
                            "$id": "return",
                            "component": "Box",
                            "domain": "function(parent){let x = parent.props.context.props.formData.return; return !parent.emptyObj(x)}",
                            "props": {
                              "uiSchema": {
                                "ui:readonly": true,
                                "ui:layout": {
                                  "component": "Grid",
                                  "props": {
                                    "container": true,
                                    "spacing": 2,
                                    "children": [
                                      {
                                        "component": "Grid",
                                        "props": {
                                          "item": true,
                                          "xs": 6,
                                          "children": [
                                            {
                                              "$id": "value_in"
                                            }
                                          ]
                                        }
                                      },
                                      {
                                        "component": "Grid",
                                        "props": {
                                          "item": true,
                                          "xs": 6,
                                          "children": [
                                            {
                                              "$id": "unit_in"
                                            }
                                          ]
                                        }
                                      },
                                      {
                                        "component": "Grid",
                                        "props": {
                                          "item": true,
                                          "xs": 12,
                                          "children": [
                                            {
                                              "$id": "results",
                                              "props": {
                                                "uiSchema": {
                                                  "ui:layout": {
                                                    "component": "datagrid",
                                                    "props": {
                                                      "density": "compact",
                                                      "columns": [
                                                        {
                                                          "field": "value_out"
                                                        },
                                                        {
                                                          "field": "unit_out"
                                                        }
                                                      ]
                                                    }
                                                  }
                                                }
                                              }
                                            }
                                          ]
                                        }
                                      }
                                    ]
                                  }
                                }
                              }
                            }
                          }
                        ]
                      }
                    }
                  ]
                }
              }
            ]
          }
        }
      ],
      "children-right": [
        {
          "component": "Stack",
          "props": {
            "direction": "row",
            "spacing": 2,
            "justifyContent": "flex-end",
            "alignItems": "center",
            "children": [
              {
                "domain": "function(parent){return !!parent.props.context.props.formContext.data}",
                "transformData": "function(parent, def){def.props.content=parent.props.context.props.formContext.data; return def}",
                "props": {
                  "id": "context-data",
                  "content": ""
                }
              }
            ]
          }
        }
      ]
    }
  },
  "ui:submitButtonOptions": {
    "norender": true
  }
}